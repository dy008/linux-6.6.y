	pinctrl-names = "default";
	pinctrl-0 = <&pcie2_0_rst>;
	status = "okay";
};	// ok


&pcie2x1l2 {
	// reset-gpios = <0xee 0x05 0x00>;
	reset-gpios = <&gpio3 RK_PA5 GPIO_ACTIVE_HIGH>;
