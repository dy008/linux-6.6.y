&i2c5 {
	clock-frequency = <200000>;
	status = "okay";

	bl5372: rtc@32 {
		compatible = "rtc,bl5372";
		reg = <0x32>;
		#clock-cells = <0>;
		clock-output-names = "hym8563";
		pinctrl-names = "default";
		pinctrl-0 = <&bl5372_int>;
		interrupt-parent = <&gpio0>;
		interrupts = <RK_PC4 IRQ_TYPE_LEVEL_LOW>;
		wakeup-source;
	};
};	// ok

&i2s0_8ch {
	pinctrl-names = "default";
	pinctrl-0 = <&i2s0_lrck
